import pandas as pd
from google.colab import files

# 파일 업로드
uploaded_files = files.upload()

# 엑셀 시트를 데이터프레임으로 읽어오기
df_apartment = pd.read_csv('행정동별_평균매매_아파트.csv', encoding='MS949')
df_office = pd.read_csv('행정동별_평균매매_오피스텔.csv', encoding='MS949')
df_multiplex = pd.read_csv('행정동별_평균매매_연립다세대.csv', encoding='MS949')

# 엑셀 시트 병합
merged_df = pd.merge(df_apartment, df_office, on=['SGG_NM', 'BJDONG_NM'], how='outer')
merged_df = pd.merge(merged_df, df_multiplex, on=['SGG_NM', 'BJDONG_NM'], how='outer')

# 결과를 엑셀로 저장
merged_df.to_excel('행정동_평균매매가격.xlsx', index=False)
